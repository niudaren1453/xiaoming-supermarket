page,view,text,swiper,swiper-item,image,navigator {
    padding: 0;
    margin: 0;
    box-sizing: border-box;
  }
page{
    --themeColor:#0382B7;
    font-size: 28rpx;
}
image {
    width: 100%;
    height: 100%;
  }
  
/* 设备状态栏 */
.status_bar{
    width: 100%;
    height: 40rpx;
    background: var(--themeColor);
}

.relative{
    width: 100%;
    height: 100%;
    position: relative;
}
